<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Launch" parent="AppTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="Dark.Overlay" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:background">?attr/colorPrimary</item>
    </style>

    <style name="Dark.Overlay.LightPopup" parent="ThemeOverlay.AppCompat.Dark">
        <!--<item name="android:background">@color/colorPrimary</item>-->
    </style>

    <style name="title">
        <item name="android:textSize">@dimen/title_size</item>
        <item name="android:textColor">@color/material_light_black</item>
        <item name="android:maxLines">2</item>
    </style>
</resources>
